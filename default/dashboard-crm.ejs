  function handleSpecialCategoryChange(checkbox) {
    const checkboxes = document.querySelectorAll('input[name="status"]');
    
    if (checkbox.checked) {
        // Disable all other checkboxes
        checkboxes.forEach(cb => {
            if (cb !== checkbox) {
                cb.disabled = true;
            }
        });
    } else {
        // Enable all checkboxes when current one is unchecked
        checkboxes.forEach(cb => {
            cb.disabled = false;
        });
